<h1>会員一覧</h1>

<table>
  <tr>
    <th>会員ID</th>
    <th>氏名</th>
    <th>メールアドレス</th>
    <th>ステータス</th>
  <tr>
  <% @members.each do |member| %>
    <tr>
      <td><%= member.id %></td>
      <td>
        <%= link_to admin_member_path(member) do %>
          <%= member.first_name %>
          <%= member.last_name %>
        <% end %>
      </td>
      <td><%= member.email %></td>
      <td><%= member.is_delete %></td>
    </tr>
  <% end %>
</table>