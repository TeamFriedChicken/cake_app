<h3>ショッピングカート</h3>

<div class="container">
  <div class="row">
    <table class="table">
      <tr>
        <th>商品名</th>
        <th>単価（税込）</th>
        <th>数量</th>
        <th>小計</th>
        <th></th>
      </tr>
      <tr>
        <% @cart_items.each do |cart_item| %>
          <%= cart_item.name %>
          <%= cart_item.price %>
          <%= cart_item.quantity %>
        <% end %>
      </tr>

    </table>
  </div>
</div>


