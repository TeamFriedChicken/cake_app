<h4>注文履歴一覧</h4>
<label>注文履歴一覧</label>

<table>
		<thead>
			<tr>
				<th>注文日</th>
				<th>配送先</th>
				<th>注文商品</th>
				<th>支払金額</th>
				<th>ステータス</th>
				<th>注文詳細</th>
			</tr>
		</thead>
	<% @orders.each do |orders| %>
		<tr>
			<td><%= order.created_at %></td>
			<td><%= order.address %></td>
			<td><%= order.name %></td>
			<td>
				<%= link_to "編集する", edit_members_delivery_address_path(delivery_address) %>
				<%= link_to "削除する", members_delivery_address_path(delivery_address), method: :delete %>
			</td>
		</tr>
	<% end %>
</table>
